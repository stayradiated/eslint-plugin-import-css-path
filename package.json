{
  "name": "eslint-plugin-import-css-path",
  "version": "2.1.0-0",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "tidy": "xo --fix",
    "build": "rm -rf ./dist && tsc",
    "test": "ava"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "ava": "4.3.3",
    "typescript": "4.8.3",
    "xo": "0.52.3"
  },
  "__comment": "The following configs are managed by `@stayradiated/jsconfig`",
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "rules": {
      "ava/assertion-arguments": "off",
      "ava/no-ignored-test-files": [
        "error",
        {
          "files": [
            "**/src/**/*.test.ts"
          ],
          "extensions": [
            "ts"
          ]
        }
      ]
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "dependencies": {
    "tsconfig-paths": "4.1.0"
  },
  "ava": {
    "verbose": true
  }
}
